{
  "name": "@auth0/auth0-react-router",
  "version": "1.0.0-beta.1",
  "description": "Auth0 Authentication SDK for React Router Applications",
  "author": "Auth0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf ./dist",
    "lint": "eslint \"./**/*.ts*\"",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run ./src/**",
    "test:e2e": "vitest run ./test/**",
    "test:ci": "vitest --watch false --coverage",
    "pretest": "npx playwright install",
    "pretest:ci": "npx playwright install"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@vitest/coverage-v8": "^3.0.6",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.35.0",
    "happy-dom": "^18.0.1",
    "nock": "^14.0.5",
    "playwright": "^1.53.2",
    "playwright-core": "^1.53.2",
    "react-router": "^7.10.1",
    "tsup": "^8.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.24.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@auth0/auth0-server-js": "^1.0.3"
  },
  "files": [
    "dist",
    "assets"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/auth0/auth0-react-router.git"
  },
  "bugs": {
    "url": "https://github.com/auth0/auth0-react-router/issues"
  },
  "homepage": "https://github.com/auth0/auth0-react-router#readme",
  "keywords": [
    "auth0"
  ]
}